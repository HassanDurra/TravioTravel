        let TotalAduts       = $("#number_of_adults");

        $(FligthSearchForm).submit(function (e) {
            if(fromCountry.val()==""){
                e.preventDefault();
                  Swal.fire({
                title: 'You Missing Something!',
                    text: 'Departure Country Is Required',
                icon: 'warning',
                confirmButtonText: 'Fill it!'
                 });
             return false; 
            }
            if (fromCity.val() == "") {
                e.preventDefault();
             
                Swal.fire({
                    title: 'You Missing Something!',

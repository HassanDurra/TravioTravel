                                    <div class="col-12 col-md-12 col-lg-5 col-xl-4">
                                        <div class="row">

                                            <div class="col-12 col-md-6 col-lg-6">
                                                <label><i class="fa text-warning fa-map-marker"></i>&nbsp; From Country<span class="text-danger" style="position:relative; bottom:3px;left:1px">*</span></label>
                                                <div class="form-group right-icon">
                                                    <select class="form-control" id="from_country" name="from_country" asp-for="from_country">
                                                        <option value="">Select Country</option>
                                                        @foreach(var country in ViewBag.country){
                                                        <option value="@country.id">@country.name</option>
                                                        }
                                                    </select>
                                                    <i class="fa fa-map-marker"></i>
                                                </div>
                                            </div><!-- end columns -->
                                              <div class="col-12 col-md-6 col-lg-6">
                                                <label><i class="fa text-warning fa-map-marker"></i>&nbsp; From City <span class="text-danger" style="position:relative; bottom:3px;left:1px">*</span></label>

                                                <div class="form-group right-icon">
                                                    <select class="form-control" id="from_city" readonly name="from_city" asp-for="from_city">
                                                        <option value="">Select City</option>
                                                    </select>
                                                    <i class="fa fa-map-marker"></i>
                                                </div>
                                            </div><!-- end columns -->
                                              <div class="col-12 col-md-6 col-lg-6">
                                                <label><i class="fa text-warning fa-map-marker"></i> &nbsp;To Country<span class="text-danger" style="position:relative; bottom:3px ; left:1px">*</span></label>

                                                <div class="form-group right-icon">
                                                    <select class="form-control" id="to_country" name="to_country" asp-for="to_country">
                                                        <option value="">Select Country</option>
                                                        @foreach (var country in ViewBag.country)
                                                        {
                                                            <option value="@country.id">@country.name</option>
                                                        }
                                                    </select>
                                                    <i class="fa fa-map-marker"></i>
                                                   
                                                </div>
                                            </div><!-- end columns -->

                                            <div class="col-12 col-md-6 col-lg-6">
                                                <label><i class="fa text-warning fa-map-marker"></i> &nbsp; To City<span class="text-danger" style="position:relative; bottom:3px ; left:1px">*</span></label>

                                                <div class="form-group right-icon">
                                                    <select class="form-control" readonly id="to_city" name="to_city" asp-for="to_city">
                                                        <option value="">Select City</option>
                                                    </select>
                                                    

                                                    <i class="fa fa-map-marker"></i>
                                                </div>
                                            </div><!-- end columns -->

                                        </div><!-- end row -->
                                    </div><!-- end columns -->

                                    <div class="col-12 col-md-12 col-lg-5 col-xl-4">
                                        <div class="row">

                                            <div class="col-12 col-md-12 col-lg-12">
                                                <label><i class="fa text-warning fa-calendar"></i> &nbsp; Departure Date<span class="text-danger" style="position:relative; bottom:3px;left:1px">*</span></label>

                                                <div class="form-group left-icon">
                                                    <input type="text" class="form-control dpd1" id="departure_date" placeholder="Departure Date" name="departure_date" asp-for="departure_date">
                                                    <i class="fa fa-calendar"></i>
                                                </div>
                                            </div><!-- end columns -->

                                          

                                        </div><!-- end row -->
                                    </div><!-- end columns -->

                                    <div class="col-12 col-md-12 col-lg-2 col-xl-2">
                                        <label> <i class="fa text-warning fa-user"></i> &nbsp;Total Adults<span class="text-danger" style="position:relative; bottom:3px;left:1px">*</span></label>

                                        <div class="form-group left-icon">
                                            <input name="number_of_adults" type="number" id="number_of_adults" placeholder="No Adults" asp-for="number_of_adults" class="form-control" />
                                            <i class="fa fa-angle-down"></i>
                                        </div>
                                    </div><!-- end columns -->

                                    <div class="col-12 col-md-12 col-lg-12 col-xl-2 search-btn mt-4" style="position:relative;top:6px">
                                        <button class="btn btn-orange searchBtn" type="submit">Search</button>
                                    </div><!-- end columns -->

                                </div><!-- end row -->
                            </form>
                        </div><!-- end flights -->

